{
  "name": "@udus/notion-libs",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./libs": {
      "import": "./dist/libs/index.js",
      "types": "./dist/libs/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "libs": [
        "dist/libs/index.d.ts"
      ]
    }
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "clean": "rimraf dist/*",
    "prebuild": "npm run clean",
    "build": "run-p build:*",
    "build:ts": "tsc",
    "watch": "run-p watch:*",
    "watch:ts": "tsc -w",
    "lint": "run-p lint:*",
    "lint:js": "run-p lint:eslint lint:prettier lint:tsc",
    "lint:eslint": "eslint src --ext .js,.jsx,.ts,.tx",
    "lint:prettier": "prettier --check src",
    "lint:tsc": "tsc --noEmit",
    "fix": "run-p fix:*",
    "fix:eslint": "eslint src --ext .js,.jsx,.ts,.tx --fix",
    "fix:prettier": "prettier --write src",
    "prepare": "husky install",
    "semantic-release": "semantic-release",
    "test": "exit 1"
  },
  "dependencies": {
    "@extractus/article-extractor": "^8.0.3",
    "@extractus/oembed-extractor": "^4.0.1",
    "@notionhq/client": "^2.2.13",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@semantic-release/git": "^10.0.1",
    "@storybook/react-vite": "^7.4.6",
    "@types/node": "^18.18.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "cpx2": "^5.0.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.5",
    "semantic-release": "^21.1.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/udus122/notion-component.git"
  },
  "publishConfig": {
    "access": "public"
  }
}